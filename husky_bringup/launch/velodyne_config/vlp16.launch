<?xml version="1.0"?>
<launch>
  <arg name="host"    default="$(optenv HUSKY_LASER_3D_HOST 192.168.131.20)" />
  <arg name="topic"   default="$(optenv HUSKY_LASER_3D_TOPIC points)" />

  <include file="$(find velodyne_pointcloud)/launch/VLP-16.launch">
    <arg name="device_ip" value="$(arg host)" />
    <remap from="velodyne_points" to="$(arg topic)" />
  </include>
</launch>
